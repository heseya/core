{
  "preset": "psr12",
  "rules": {
    "not_operator_with_space": false,
    "simplified_null_return": true,
    "array_push": true,
    "concat_space": {
      "spacing": "one"
    },
    "no_useless_concat_operator": {
      "juggle_simple_strings": true
    },
    "new_with_braces": {
      "anonymous_class": false,
      "named_class": true
    }
  }
}
